{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "src": "azrmps/3.6.0",
        "version": "azurermps-3.6.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ],
        "dest": "module"
      },
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "src": "azrmps/conceptual/3.6.0",
        "version": "azurermps-3.6.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ],
        "dest": "azure"
      },
      {
        "files": [
          "*.md",
          "*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "module.page.json",
          "codesnippet/**",
          "langwordmapping/**"
        ],
        "exclude": [
          "_site/**",
          "**/obj/**",
          "**.meta"
        ],
        "src": "azrmps/3.6.0",
        "version": "azurermps-3.6.0",
        "dest": "module"
      }
    ],
    "versions": {
      "azurermps-3.6.0": {
        "dest": "azurermps-3.6.0"
      }
    },
    "exportRawModel": true,
    "globalMetadata": {
      "showPowerShellPicker": "true"
    },
    "dest": "testMultipleVersion"
  }
}
